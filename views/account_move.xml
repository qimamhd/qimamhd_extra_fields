<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_account_move_form_extra_fields_tab" model="ir.ui.view">
        <field name="name">account.move.form.extra.fields.tab</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
           <xpath expr="//notebook" position="inside">
				<page string="Extra Fields" name="extra_fields_tab">
					<group>
						<field name="x_extra_text_1"/>
						<field name="x_extra_text_2"/>
						<field name="x_extra_number_1"/>
						<field name="x_extra_date_1"/>
					</group>
				</page>
			</xpath>
        </field>
    </record>

</odoo>